{
  "name": "react-examples",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.11.0",
    "react-scripts": "^1.1.4"
  },
  "dependencies": {
    "cross-fetch": "^2.2.1",
    "node-sass": "^4.9.2",
    "react": "^16.4.1",
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.2.0",
    "sanitize.css": "^5.0.0"
  },
  "scripts": {
    "start": "npm-run-all -p sass:watch start-js",
    "start-js": "react-app-rewired start",
    "build": "npm-run-all sass:build build-js",
    "build-js": "react-app-rewired build",
    "sass:watch": "node-sass -w src/styles/scss -o src/styles/css",
    "sass:build":
      "node-sass src/styles/scss -o src/styles/css --source-map true",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "homepage": "https://cra-redux-router-thunk.herokuapp.com",
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  }
}
